<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/btnBorder">
        <shape android:shape="rectangle">
            <corners android:radius="4dip" />
            <solid android:color="@color/darkTransparent" />
        </shape>
    </item>
    <item
        android:id="@+id/btnBack"
        android:bottom="1dip"
        android:left="1dip"
        android:right="1dip"
        android:top="1dip">
        <shape android:shape="rectangle">
            <corners android:radius="4dip" />
            <solid android:color="@color/bottomNavigationBar" />
        </shape>
    </item>
</layer-list>